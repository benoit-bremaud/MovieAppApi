{
  "metadata": [
    {
      "src": [
        {
          "files": ["MovieAppApi.csproj"],
          "src": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml"],
        "exclude": ["_site/**"]
      },
      {
        "files": ["toc.yml"]
      }
    ],
    "resource": [
      {
        "files": ["images/**", "styles/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["_site/**"]
      }
    ],
    "globalMetadata": {
      "_appTitle": "MovieApp API Documentation"
    },
    "dest": "_site"
  }
}
